<title>{{ page_render_title() }}</title>

<meta name="description" content="{{ page_render_meta_description() }}" />
<meta name="keywords" content="{{ page_render_meta_keywords() }}" />

{% if page_og_enabled() %}
    {% if page_og_type() %}
        <meta property="og:type" content="{{ page_og_type() }}" />
    {% endif %}
    {% if page_og_site_name() %}
        <meta property="og:site_name" content="{{ page_og_site_name() }}" />
    {% endif %}
{% endif %}